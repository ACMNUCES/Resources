#include <stdio.h>

int xpower (int x, int a) {
	if (a == 1) {
		return x;
	}
	return x * xpower (x, a - 1);
}

int find_S (int x) {
	int s = 0;
	while (x != 0) {
		s += x % 10;
		x /= 10;
	}
	return s;	
}

int eq (int a, int b, int c, int s, int x) {
	return b * find_S (xpower (x, a)) + c;
}

int main () {
	int a, b, c, s, x, solC, i;
	scanf ("%d %d %d", &a, &b, &c);
	for (x = 1, solC = 0; x < 10000; x++) {
		if (x == eq (a, b, c, s, x)) {
			solC++;
		}
	}
	printf ("%d\n", solC);
	for (x = 1, i = 0; x < 10000; x++) {
		if (x == eq (a, b, c, s, x)) {
			solC--;
			printf ("%d", x);
			if (solC > 0) {
				printf (" ");
			}
			else {
				break;
			}
		}
	}
}
